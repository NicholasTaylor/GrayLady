{

    "manifest_version": 2,
    "name": "GrayLady",
    "version": "1.7",

    "description": "Expands access on certain news websites of note.",

    "icons": {
        "48": "icons/graylady-48.png",
        "96": "icons/graylady-96.png"
    },

    "content_scripts": [
      {
        "matches": ["*://*.nytimes.com/*", "*://*.washingtonpost.com/*", "*://*.theatlantic.com/*"],
        "js": ["graylady.js"]
      }
    ]

  }
